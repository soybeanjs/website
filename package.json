{
  "name": "soybeanjs",
  "version": "0.0.1",
  "private": true,
  "description": "SoybeanJS's website",
  "author": {
    "name": "Soybean",
    "email": "honghuangdc@gmail.com",
    "url": "https://github.com/honghuangdc"
  },
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --fix",
    "tsx": "tsx",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy update-pkg",
    "update-version": "bumpp package.json packages/*/package.json",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm update-version && pnpm publish-pkg",
    "prepare": "pnpm -r run stub"
  },
  "devDependencies": {
    "@astrojs/mdx": "^0.16.0",
    "@astrojs/react": "^2.0.2",
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/solid-js": "^2.0.2",
    "@astrojs/svelte": "^2.0.1",
    "@astrojs/vue": "^2.0.1",
    "@soybeanjs/cli": "^0.1.6",
    "@unocss/astro": "^0.49.4",
    "@unocss/preset-uno": "^0.49.4",
    "astro": "^2.0.6",
    "astro-eslint-parser": "^0.11.0",
    "eslint": "^8.33.0",
    "eslint-config-soybeanjs": "^0.2.3",
    "eslint-config-soybeanjs-svelte": "^0.2.3",
    "eslint-config-soybeanjs-vue": "^0.2.3",
    "eslint-plugin-astro": "^0.23.0",
    "lint-staged": "^13.1.0",
    "prettier-plugin-astro": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-git-hooks": "^2.8.1",
    "solid-js": "^1.6.10",
    "svelte": "^3.55.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "eslint . --fix"
    ]
  }
}
